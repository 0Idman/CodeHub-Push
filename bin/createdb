#!/usr/bin/env node
'use strict';
const db = require('../lib/db');
const path = require('path');
const co = require('co');

co(function *() {
  yield db.load(path.join(__dirname, '..', 'database.db'));
  yield db.sync();
  process.exit(0);
}).catch(err => {
  console.error(err);
  process.exit(-1);
});
